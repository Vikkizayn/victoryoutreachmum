{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "version": 2,
  "builds": [{ "src": "frontend/package.json", "use": "@vercel/static-build" }],
  "routes": [{ "src": "/(.*)", "dest": "frontend/dist/$1" }]
}
